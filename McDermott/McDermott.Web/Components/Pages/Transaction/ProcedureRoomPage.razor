@page "/clinic-service/procedure-room"
@rendermode InteractiveServer

<PageTitle>McHealthCare - Procedure Room</PageTitle>

@if (IsAccess)
{
    <div class="wrapper">

        <BaseHeaderPage Title="Procedure Room" OnClickBack="@(async () => await LoadData())" ShowBackButton="ShowForm" />

        <section class="content">
            <div class="container-fluid">
                <div class="row">
                    <div class="col">
                        @if (!ShowForm)
                        {
                            <div class="card">
                                <DxLoadingPanel @bind-Visible="PanelVisible"
                                                IndicatorAnimationType="WaitIndicatorAnimationType.Pulse"
                                                IsContentBlocked="true"
                                                ApplyBackgroundShading="true"
                                                IndicatorAreaVisible="false"
                                                Text="Fetching Data...">
                                    <MyGrid @ref="Grid"
                                            Data="GeneralConsultanMedicalSupports"
                                            @bind-SelectedDataItems="@SelectedDataItems"
                                            EditModelSaving="OnSave"
                                            DataItemDeleting="OnDelete"
                                            FocusedRowChanged="Grid_FocusedRowChanged"
                                            KeyFieldName="Id"
                                            RowDoubleClick="EditItem_Click">
                                        <ToolbarTemplate>
                                            <MyDxToolbarBase TItem="GeneralConsultanMedicalSupportDto"
                                                             Items="@GeneralConsultanMedicalSupports"
                                                             SelectedDataItems="@SelectedDataItems"
                                                             NewItem_Click="@NewConfinedSpace"
                                                             BtnNewName="Create Confined Space"
                                                             EditItem_Click="@EditItem_Click"
                                                             DeleteItem_Click="@DeleteItem_Click"
                                                             Refresh_Click="@Refresh_Click"
                                                             IsImport="false"
                                                             VisibleNew="true"
                                                             Grid="Grid"
                                                             VisibleDelete="UserAccessCRUID.Delete"
                                                             VisibleEdit="UserAccessCRUID.Update" />
                                        </ToolbarTemplate>
                                        <Columns>
                                            <DxGridSelectionColumn Width="15px" />
                                            @* <DxGridDataColumn FieldName="GeneralConsultanService.RegistrationDate" DisplayFormat="ddd, dd MMM yyyy" Caption="Registration Date" /> *@
                                            <DxGridDataColumn FieldName="GeneralConsultanService.Patient.Name" Caption="Patient" />
                                            <DxGridDataColumn FieldName="ConfinedSpaceString" Caption="Type" />
                                            <DxGridDataColumn FieldName="StatusName" Caption="Status" />
                                        </Columns>
                                    </MyGrid>
                                </DxLoadingPanel>
                            </div>
                        }
                        else
                        {
                            @if (!GeneralConsultanMedicalSupport.IsConfinedSpace)
                            {

                                <div class="row justify-content-end mb-3">
                                    <div class="col-auto align-self-end">
                                        <DxTabs>
                                            <DxTab Text="Draft" Enabled="@IsStatus(EnumStatusGeneralConsultantServiceProcedureRoom.Draft)"></DxTab>
                                            <DxTab Text="In-Progress" Enabled="@IsStatus(EnumStatusGeneralConsultantServiceProcedureRoom.InProgress)"></DxTab>
                                            <DxTab Text="Finish" Enabled="@IsStatus(EnumStatusGeneralConsultantServiceProcedureRoom.Finish)"></DxTab>
                                        </DxTabs>
                                    </div>
                                </div>

                                @if (GeneralConsultanMedicalSupport.Status is not null && !GeneralConsultanMedicalSupport.Status.Equals(EnumStatusGeneralConsultantServiceProcedureRoom.Finish))
                                {
                                    <div class="card">
                                        <div class="card-body">
                                            <div class="w-100">
                                                <div class="col align-self-center">
                                                    <DxButton IconCssClass="fa-solid fa-check" Text="@StagingText" Click="OnClickConfirm" RenderStyle="ButtonRenderStyle.Primary" Enabled="@(!GeneralConsultanMedicalSupport.Status.Equals(EnumStatusGeneralConsultantServiceProcedureRoom.Finish) && !Loading)"></DxButton>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                }
                            }

                            @if (GeneralConsultanMedicalSupport.Status is not null && !GeneralConsultanMedicalSupport.Status.Equals(EnumStatusGeneralConsultantServiceProcedureRoom.Finish))
                            {
                                <div class="card ">
                                    <div class="card-body">
                                        <div class="w-100">
                                            <div class="row w-100">
                                                <div class="col-md-12 align-self-center">
                                                    <DxButton RenderStyle="ButtonRenderStyle.Primary" RenderStyleMode="@ButtonRenderStyleMode.Contained" IconCssClass="fa-solid fa-floppy-disk" Click="OnSave" Text="Save" SubmitFormOnClick="true"></DxButton>
                                                    <DxButton RenderStyle="ButtonRenderStyle.Danger" RenderStyleMode="@ButtonRenderStyleMode.Contained" IconCssClass="fa-solid fa-xmark" Text="Discard" Click="OnCancel"></DxButton>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            }
                            else if (GeneralConsultanMedicalSupport.IsConfinedSpace)
                            {
                                <div class="card ">
                                    <div class="card-body">
                                        <div class="w-100">
                                            <div class="row w-100">
                                                <div class="col-md-12 align-self-center">
                                                    <DxButton RenderStyle="ButtonRenderStyle.Primary" RenderStyleMode="@ButtonRenderStyleMode.Contained" IconCssClass="fa-solid fa-floppy-disk" Click="OnSave" Text="Save" SubmitFormOnClick="true"></DxButton>
                                                    <DxButton RenderStyle="ButtonRenderStyle.Danger" RenderStyleMode="@ButtonRenderStyleMode.Contained" IconCssClass="fa-solid fa-xmark" Text="Discard" Click="OnCancel"></DxButton>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            }

                            <div class="w-100">
                                <div class="w-100">
                                    <div class="card">
                                        <div class="card-header">
                                            <span class="fw-bolder">Personal Data</span>
                                        </div>

                                        <div class="card-body">
                                            <div class="row">
                                                @{
                                                    var yearsWorked = "-";
                                                    if (GeneralConsultanService.Patient is null)
                                                    {
                                                        GeneralConsultanService.Patient = new();
                                                    }
                                                    if (GeneralConsultanService.Patient.Department is null)
                                                    {
                                                        GeneralConsultanService.Patient.Department = new();
                                                    }
                                                    if (GeneralConsultanService.Patient.IdCardCountry is null)
                                                    {
                                                        GeneralConsultanService.Patient.IdCardCountry = new();
                                                    }

                                                    if (GeneralConsultanService.Patient.JoinDate is not null)
                                                    {
                                                        yearsWorked = $"{DateTime.Now.Year - GeneralConsultanService.Patient.JoinDate.GetValueOrDefault().Year} Years";
                                                    }
                                                }

                                                @{
                                                    var yearsWorked2 = "-";
                                                    if (GeneralConsultanMedicalSupport.Employee is null)
                                                    {
                                                        GeneralConsultanMedicalSupport.Employee = new();
                                                    }
                                                    if (GeneralConsultanMedicalSupport.Employee.Department is null)
                                                    {
                                                        GeneralConsultanMedicalSupport.Employee.Department = new();
                                                    }
                                                    if (GeneralConsultanMedicalSupport.Employee.IdCardCountry is null)
                                                    {
                                                        GeneralConsultanMedicalSupport.Employee.IdCardCountry = new();
                                                    }

                                                    if (GeneralConsultanService.Patient.JoinDate is not null)
                                                    {
                                                        yearsWorked2 = $"{DateTime.Now.Year - GeneralConsultanMedicalSupport.Employee.JoinDate.GetValueOrDefault().Year} Years";
                                                    }
                                                }
                                                @if (!GeneralConsultanMedicalSupport.IsConfinedSpace)
                                                {

                                                    <div class="col-md-6">
                                                        <DxFormLayout>

                                                            <DxFormLayoutItem Caption="Name" CaptionCssClass="normal-caption" ColSpanMd="12">
                                                                <MyTextBox @bind-Text="@GeneralConsultanService.Patient.Name" ReadOnly
                                                                           NullText="Patient Name" />
                                                            </DxFormLayoutItem>

                                                            <DxFormLayoutItem Caption="Dept." CaptionCssClass="normal-caption" ColSpanMd="12">
                                                                <MyTextBox @bind-Text="@GeneralConsultanService.Patient.Department.Name" ReadOnly
                                                                           NullText="Dept." />
                                                            </DxFormLayoutItem>

                                                            <DxFormLayoutItem Caption="Company" CaptionCssClass="normal-caption" ColSpanMd="12">
                                                                <MyTextBox @bind-Text="@GeneralConsultanService.Patient.Department.Name" ReadOnly
                                                                           NullText="Company" />
                                                            </DxFormLayoutItem>
                                                        </DxFormLayout>
                                                    </div>
                                                    <div class="col-md-6">
                                                        <DxFormLayout>

                                                            <DxFormLayoutItem Caption="Nationality" CaptionCssClass="normal-caption" ColSpanMd="12">
                                                                <MyTextBox @bind-Text="@GeneralConsultanService.Patient.IdCardCountry.Name" ReadOnly
                                                                           NullText="Nationality" />
                                                            </DxFormLayoutItem>

                                                            <DxFormLayoutItem Caption="Employee ID" CaptionCssClass="normal-caption" ColSpanMd="12">
                                                                <MyTextBox @bind-Text="@GeneralConsultanService.Patient.NIP" ReadOnly
                                                                           NullText="Employee ID" />
                                                            </DxFormLayoutItem>

                                                            <DxFormLayoutItem Caption="Years Worked" CaptionCssClass="normal-caption" ColSpanMd="12">
                                                                <MyTextBox @bind-Text="@yearsWorked" ReadOnly
                                                                           NullText="Years Worked" />
                                                            </DxFormLayoutItem>
                                                        </DxFormLayout>
                                                    </div>
                                                }
                                                else
                                                {
                                                    <div class="col-md-6">
                                                        <DxFormLayout>
                                                            <DxFormLayoutItem Caption="Name" CaptionCssClass="normal-caption" ColSpanMd="12">
                                                                <DxComboBox Data="@Employees"
                                                                            @bind-Value="@GeneralConsultanMedicalSupport.EmployeeId"
                                                                            FilteringMode="@DataGridFilteringMode.Contains"
                                                                            NullText="Select Name..."
                                                                            TextFieldName="Name"
                                                                            ValueFieldName="Id"
                                                                            SelectedItemChanged="@((UserDto freq)=>OnSelectEmployee(freq))"
                                                                            ClearButtonDisplayMode="DataEditorClearButtonDisplayMode.Auto"
                                                                            ShowValidationIcon="true" />
                                                            </DxFormLayoutItem>

                                                            <DxFormLayoutItem Caption="Dept." CaptionCssClass="normal-caption" ColSpanMd="12">
                                                                <MyTextBox @bind-Text="@GeneralConsultanMedicalSupport.Employee.Department.Name" ReadOnly
                                                                           NullText="Dept." />
                                                            </DxFormLayoutItem>

                                                            <DxFormLayoutItem Caption="Company" CaptionCssClass="normal-caption" ColSpanMd="12">
                                                                <MyTextBox @bind-Text="@GeneralConsultanMedicalSupport.Employee.Department.Name" ReadOnly
                                                                           NullText="Company" />
                                                            </DxFormLayoutItem>
                                                        </DxFormLayout>
                                                    </div>
                                                    <div class="col-md-6">
                                                        <DxFormLayout>

                                                            <DxFormLayoutItem Caption="Nationality" CaptionCssClass="normal-caption" ColSpanMd="12">
                                                                <MyTextBox @bind-Text="@GeneralConsultanMedicalSupport.Employee.IdCardCountry.Name" ReadOnly
                                                                           NullText="Nationality" />
                                                            </DxFormLayoutItem>

                                                            <DxFormLayoutItem Caption="Employee ID" CaptionCssClass="normal-caption" ColSpanMd="12">
                                                                <MyTextBox @bind-Text="@GeneralConsultanMedicalSupport.Employee.NIP" ReadOnly
                                                                           NullText="Employee ID" />
                                                            </DxFormLayoutItem>

                                                            <DxFormLayoutItem Caption="Years Worked" CaptionCssClass="normal-caption" ColSpanMd="12">
                                                                <MyTextBox @bind-Text="@yearsWorked2" ReadOnly
                                                                           NullText="Years Worked" />
                                                            </DxFormLayoutItem>
                                                        </DxFormLayout>
                                                    </div>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="row justify-content-end mb-3">
                                <div class="col-auto align-self-end">
                                    <DxTabs>
                                        <DxTab Text="Referral from GC" Enabled="@(GeneralConsultanMedicalSupport.IsConfinedSpace == false)"></DxTab>
                                        <DxTab Text="Confined Space" Enabled="@GeneralConsultanMedicalSupport.IsConfinedSpace"></DxTab>
                                    </DxTabs>
                                </div>
                            </div>

                            @if (GeneralConsultanMedicalSupport.IsConfinedSpace)
                            {
                                <DxLoadingPanel @bind-Visible="Loading"
                                                IndicatorAnimationType="WaitIndicatorAnimationType.Pulse"
                                                IsContentBlocked="true"
                                                ApplyBackgroundShading="true"
                                                IndicatorAreaVisible="false"
                                                Text="Fetching Data...">
                                    <div class="w-100">
                                        <div class="w-100">
                                            <div class="card">
                                                <div class="card-header">
                                                    <span class="fw-bolder">Medical History</span>
                                                </div>

                                                <div class="card-body">
                                                    <div class="row">
                                                        <div class="col-md-6">
                                                            <DxFormLayout>
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="First time entering confined space" ColSpanMd="12">
                                                                    <DxRadioGroup Items="@Options"
                                                                                  TextFieldName="Text"
                                                                                  ValueFieldName="Value"
                                                                                  @bind-Value="@GeneralConsultanMedicalSupport.IsFirstTimeEnteringConfinedSpace"
                                                                                  Layout="@RadioGroupLayout.Horizontal" />
                                                                </DxFormLayoutItem>

                                                                <DxFormLayoutItem Visible="!GeneralConsultanMedicalSupport.IsFirstTimeEnteringConfinedSpace" CaptionCssClass="normal-caption" Caption="If no, How many times">
                                                                    <DxSpinEdit ShowValidationIcon="true"
                                                                                MinValue="1" @bind-Value="@GeneralConsultanMedicalSupport.EnteringConfinedSpaceCount"
                                                                                NullText="0"
                                                                                ClearButtonDisplayMode="DataEditorClearButtonDisplayMode.Auto" />
                                                                </DxFormLayoutItem>

                                                                <!-- Item 1 -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="1. Defective sense of smell" ColSpanMd="12">
                                                                    <DxRadioGroup Items="@Options"
                                                                                  TextFieldName="Text"
                                                                                  ValueFieldName="Value"
                                                                                  @bind-Value="@GeneralConsultanMedicalSupport.IsDefectiveSenseOfSmell"
                                                                                  Layout="@RadioGroupLayout.Horizontal" />
                                                                </DxFormLayoutItem>

                                                                <!-- Item 2 -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="2. Asthma or Lung Ailment" ColSpanMd="12">
                                                                    <DxRadioGroup Items="@Options"
                                                                                  TextFieldName="Text"
                                                                                  ValueFieldName="Value"
                                                                                  @bind-Value="@GeneralConsultanMedicalSupport.IsAsthmaOrLungAilment"
                                                                                  Layout="@RadioGroupLayout.Horizontal" />
                                                                </DxFormLayoutItem>

                                                                <!-- Item 3 -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="3. Back Pain or Limitation of Mobility" ColSpanMd="12">
                                                                    <DxRadioGroup Items="@Options"
                                                                                  TextFieldName="Text"
                                                                                  ValueFieldName="Value"
                                                                                  @bind-Value="@GeneralConsultanMedicalSupport.IsBackPainOrLimitationOfMobility"
                                                                                  Layout="@RadioGroupLayout.Horizontal" />
                                                                </DxFormLayoutItem>

                                                                <!-- Item 4 -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="4. Claustrophobia (fear of enclosure)" ColSpanMd="12">
                                                                    <DxRadioGroup Items="@Options"
                                                                                  TextFieldName="Text"
                                                                                  ValueFieldName="Value"
                                                                                  @bind-Value="@GeneralConsultanMedicalSupport.IsClaustrophobia"
                                                                                  Layout="@RadioGroupLayout.Horizontal" />
                                                                </DxFormLayoutItem>

                                                                <!-- Item 5 -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="5. Diabetes / Hypoglycemia" ColSpanMd="12">
                                                                    <DxRadioGroup Items="@Options"
                                                                                  TextFieldName="Text"
                                                                                  ValueFieldName="Value"
                                                                                  @bind-Value="@GeneralConsultanMedicalSupport.IsDiabetesOrHypoglycemia"
                                                                                  Layout="@RadioGroupLayout.Horizontal" />
                                                                </DxFormLayoutItem>

                                                                <!-- Item 6 -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="6. Eyesight Problem" ColSpanMd="12">
                                                                    <DxRadioGroup Items="@Options"
                                                                                  TextFieldName="Text"
                                                                                  ValueFieldName="Value"
                                                                                  @bind-Value="@GeneralConsultanMedicalSupport.IsEyesightProblem"
                                                                                  Layout="@RadioGroupLayout.Horizontal" />
                                                                </DxFormLayoutItem>
                                                            </DxFormLayout>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <DxFormLayout>
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" ColSpanMd="12">

                                                                </DxFormLayoutItem>

                                                                <DxFormLayoutItem Visible="GeneralConsultanMedicalSupport.IsFirstTimeEnteringConfinedSpace" CaptionCssClass="normal-caption">

                                                                </DxFormLayoutItem>

                                                                <!-- Item 7 -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="7. Fainting Spell/Seizure/Epilepsy" ColSpanMd="12">
                                                                    <DxRadioGroup Items="@Options"
                                                                                  TextFieldName="Text"
                                                                                  ValueFieldName="Value"
                                                                                  @bind-Value="@GeneralConsultanMedicalSupport.IsFaintingSpellOrSeizureOrEpilepsy"
                                                                                  Layout="@RadioGroupLayout.Horizontal" />
                                                                </DxFormLayoutItem>

                                                                <!-- Item 8 -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="8. Hearing Disorder" ColSpanMd="12">
                                                                    <DxRadioGroup Items="@Options"
                                                                                  TextFieldName="Text"
                                                                                  ValueFieldName="Value"
                                                                                  @bind-Value="@GeneralConsultanMedicalSupport.IsHearingDisorder"
                                                                                  Layout="@RadioGroupLayout.Horizontal" />
                                                                </DxFormLayoutItem>

                                                                <!-- Item 9 -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="9. Heart Disease or Disorder" ColSpanMd="12">
                                                                    <DxRadioGroup Items="@Options"
                                                                                  TextFieldName="Text"
                                                                                  ValueFieldName="Value"
                                                                                  @bind-Value="@GeneralConsultanMedicalSupport.IsHeartDiseaseOrDisorder"
                                                                                  Layout="@RadioGroupLayout.Horizontal" />
                                                                </DxFormLayoutItem>

                                                                <!-- Item 10 -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="10. High Blood Pressure" ColSpanMd="12">
                                                                    <DxRadioGroup Items="@Options"
                                                                                  TextFieldName="Text"
                                                                                  ValueFieldName="Value"
                                                                                  @bind-Value="@GeneralConsultanMedicalSupport.IsHighBloodPressure"
                                                                                  Layout="@RadioGroupLayout.Horizontal" />
                                                                </DxFormLayoutItem>

                                                                <!-- Item 11 -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="11. Lower Limbs Deformity" ColSpanMd="12">
                                                                    <DxRadioGroup Items="@Options"
                                                                                  TextFieldName="Text"
                                                                                  ValueFieldName="Value"
                                                                                  @bind-Value="@GeneralConsultanMedicalSupport.IsLowerLimbsDeformity"
                                                                                  Layout="@RadioGroupLayout.Horizontal" />
                                                                </DxFormLayoutItem>

                                                                <!-- Item 12 -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="12. Meniere's Disease or Vertigo" ColSpanMd="12">
                                                                    <DxRadioGroup Items="@Options"
                                                                                  TextFieldName="Text"
                                                                                  ValueFieldName="Value"
                                                                                  @bind-Value="@GeneralConsultanMedicalSupport.IsMeniereDiseaseOrVertigo"
                                                                                  Layout="@RadioGroupLayout.Horizontal" />
                                                                </DxFormLayoutItem>

                                                            </DxFormLayout>
                                                        </div>

                                                        <div class="col-md-12">
                                                            <DxFormLayout>
                                                                <DxFormLayoutItem Caption="Remarks on the above, if any, otherwise write NIL:" CaptionPosition="CaptionPosition.Vertical" CaptionCssClass="normal-caption" ColSpanMd="12">
                                                                    <DxMemo @bind-Text="GeneralConsultanMedicalSupport.RemarksMedicalHistory"
                                                                            Rows="5"
                                                                            TextAreaCssClass="w-100"
                                                                            CssClass="w-100"
                                                                            NullText="Remarks on the above, if any, otherwise write NIL" />
                                                                </DxFormLayoutItem>
                                                            </DxFormLayout>
                                                        </div>

                                                        <div class="col-md-6">

                                                            <DxFormLayout CssClass="border border-2 rounded-3 pb-2 mt-2">
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" CaptionPosition="CaptionPosition.Vertical" Caption="I declare that all the Information's above to be true to the best of my knowledge and belief" ColSpanMd="12">

                                                                </DxFormLayoutItem>

                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" ColSpanMd="6">
                                                                    <DxDateEdit NullText="Date" @bind-Date="@GeneralConsultanMedicalSupport.DateMedialHistory" />
                                                                </DxFormLayoutItem>

                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" ColSpanMd="12">
                                                                    <SignaturePad.SignaturePad Class="border border-2 rounded-3" @bind-Value="GeneralConsultanMedicalSupport.SignatureEmployeeImagesMedicalHistory" ClearButtonClass="btn btn-info" ShowClearButton="true" Options="_options" style="width: 100%" />
                                                                </DxFormLayoutItem>

                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" ColSpanMd="6">
                                                                    <DxComboBox Data="@Employees"
                                                                                @bind-Value="@GeneralConsultanMedicalSupport.SignatureEmployeeId"
                                                                                FilteringMode="@DataGridFilteringMode.Contains"
                                                                                NullText="Select Employee..."
                                                                                TextFieldName="Name"
                                                                                ValueFieldName="Id"
                                                                                SelectedItemChanged="@((UserDto freq)=>OnSelectEmployee(freq))"
                                                                                ClearButtonDisplayMode="DataEditorClearButtonDisplayMode.Auto"
                                                                                ShowValidationIcon="true" />
                                                                </DxFormLayoutItem>
                                                            </DxFormLayout>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="card">
                                                <div class="card-header">
                                                    <span class="fw-bolder">Physical Examination</span>
                                                </div>

                                                <div class="card-body">
                                                    <div class="row">
                                                        <div class="col-md-6">
                                                            <DxFormLayout>
                                                                <!-- Weight -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="Weight" ColSpanMd="12">
                                                                    <DxSpinEdit ShowValidationIcon="true"
                                                                                MinValue="1"
                                                                                @bind-Value="@GeneralConsultanMedicalSupport.Wt"
                                                                                NullText="0"
                                                                                ClearButtonDisplayMode="DataEditorClearButtonDisplayMode.Auto" />
                                                                </DxFormLayoutItem>

                                                                <!-- Blood Pressure -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="BP" ColSpanMd="12">
                                                                    <DxSpinEdit ShowValidationIcon="true"
                                                                                MinValue="1"
                                                                                @bind-Value="@GeneralConsultanMedicalSupport.Bp"
                                                                                NullText="0"
                                                                                ClearButtonDisplayMode="DataEditorClearButtonDisplayMode.Auto" />
                                                                </DxFormLayoutItem>

                                                                <!-- Height -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="Height" ColSpanMd="12">
                                                                    <DxSpinEdit ShowValidationIcon="true"
                                                                                MinValue="1"
                                                                                @bind-Value="@GeneralConsultanMedicalSupport.Height"
                                                                                NullText="0"
                                                                                ClearButtonDisplayMode="DataEditorClearButtonDisplayMode.Auto" />
                                                                </DxFormLayoutItem>

                                                                <!-- Pulse -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="Pulse" ColSpanMd="12">
                                                                    <DxSpinEdit ShowValidationIcon="true"
                                                                                MinValue="1"
                                                                                @bind-Value="@GeneralConsultanMedicalSupport.Pulse"
                                                                                NullText="0"
                                                                                ClearButtonDisplayMode="DataEditorClearButtonDisplayMode.Auto" />
                                                                </DxFormLayoutItem>
                                                            </DxFormLayout>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <DxFormLayout>
                                                                <!-- Chest Circumference -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="Chest Circumference" ColSpanMd="12">
                                                                    <DxSpinEdit ShowValidationIcon="true"
                                                                                MinValue="1"
                                                                                @bind-Value="@GeneralConsultanMedicalSupport.ChestCircumference"
                                                                                NullText="0"
                                                                                ClearButtonDisplayMode="DataEditorClearButtonDisplayMode.Auto" />
                                                                </DxFormLayoutItem>

                                                                <!-- Abdomen Circumference -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="Abd. Circumference" ColSpanMd="12">
                                                                    <DxSpinEdit ShowValidationIcon="true"
                                                                                MinValue="1"
                                                                                @bind-Value="@GeneralConsultanMedicalSupport.AbdomenCircumference"
                                                                                NullText="0"
                                                                                ClearButtonDisplayMode="DataEditorClearButtonDisplayMode.Auto" />
                                                                </DxFormLayoutItem>

                                                                <!-- Respiratory Rate -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="Respiratory Rate" ColSpanMd="12">
                                                                    <DxSpinEdit ShowValidationIcon="true"
                                                                                MinValue="1"
                                                                                @bind-Value="@GeneralConsultanMedicalSupport.RespiratoryRate"
                                                                                NullText="0"
                                                                                ClearButtonDisplayMode="DataEditorClearButtonDisplayMode.Auto" />
                                                                </DxFormLayoutItem>

                                                                <!-- Temperature -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="Temperature" ColSpanMd="12">
                                                                    <DxSpinEdit ShowValidationIcon="true"
                                                                                MinValue="1"
                                                                                @bind-Value="@GeneralConsultanMedicalSupport.Temperature"
                                                                                NullText="0"
                                                                                ClearButtonDisplayMode="DataEditorClearButtonDisplayMode.Auto" />
                                                                </DxFormLayoutItem>
                                                            </DxFormLayout>
                                                        </div>

                                                        <div class="col-md-12 mt-3 justify-content-center align-items-center text-center ">
                                                            <label class="center-label">PERTINENT P.E. FINDINGS</label>
                                                        </div>

                                                        <div class="col-md-12">
                                                            <DxFormLayout>
                                                                <DxFormLayoutItem Caption="EYE" CaptionCssClass="normal-caption" ColSpanMd="12">
                                                                    <DxTextBox @bind-Text="@GeneralConsultanMedicalSupport.Eye" NullText="EYE" />
                                                                </DxFormLayoutItem>

                                                                <DxFormLayoutItem Caption="EAR NOSE THROAT" CaptionCssClass="normal-caption" ColSpanMd="12">
