        <MailMergeSettings>
            <DxMailMergeSettings ViewMergedData="true"></DxMailMergeSettings>
        </MailMergeSettings>
    </DxRichEdit>
</DxPopup>

<DxPopup Visible="isAccident" Width="800px" Shown="OnPopupShown" CloseOnOutsideClick="false" ShowCloseButton="true" Closed="ClosePopUpAccident">
    <div class="card">
        <div class="card-body">
            <DxFormLayout CssClass="w-100 justify-content-center">
                <DxFormLayoutItem Caption="location of the marking points" CaptionPosition="CaptionPosition.Vertical" ColSpanMd="12">
                    <canvas id="MyCanvas" width="700" height="600" style="border: 1px solid black; align-content:center"></canvas>
                </DxFormLayoutItem>
                <DxFormLayoutItem>
                    <button class="btn btn-sm btn-primary" @onclick="ResetMarkers">Reset</button>
                    <button class="btn btn-sm btn-success" @onclick="SaveMarkers">Save</button>
                </DxFormLayoutItem>
            </DxFormLayout>
            <DxFormLayout CssClass="w-100">
                <DxFormLayoutItem Caption="Description" CaptionPosition="CaptionPosition.Vertical">
                    <DxMemo @bind-Text="description"></DxMemo>
                </DxFormLayoutItem>
            </DxFormLayout>
        </div>
    </div>
</DxPopup>


