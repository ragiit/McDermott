                                                                                                          CssClass="w-20 ml" />
                                                                                                <DxButton Text="Clear"
                                                                                                          RenderStyle="ButtonRenderStyle.Danger"
                                                                                                          Click="OnClickClearCPPT"
                                                                                                          IconCssClass="fa-solid fa-xmark"
                                                                                                          CssClass="w-20 ml" />
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>

                                                                <hr />
                                                            }

                                                            <div class="w-100 mt-2">
                                                                <div class="card w-100">
                                                                    <div class="card-header">
                                                                        <span class="fw-bolder">CPPT</span>
                                                                    </div>

                                                                    <div class="row w-100">
                                                                        <div class="col-md-12 w-100">
                                                                            <DxFormLayout CssClass="w-100">
                                                                                <DxLoadingPanel @bind-Visible="PanelVisible"
                                                                                                IndicatorAnimationType="WaitIndicatorAnimationType.Pulse"
                                                                                                IsContentBlocked="true"
                                                                                                ApplyBackgroundShading="true"
                                                                                                IndicatorAreaVisible="false"
                                                                                                Text="Fetching Data...">
                                                                                    <MyGrid @ref="GridCppt"
                                                                                            Data="GeneralConsultanCPPTs"
                                                                                            @bind-SelectedDataItems="@SelectedDataItemsCPPT"
                                                                                            EditModelSaving="OnClickConfirmCPPT"
                                                                                            DataItemDeleting="OnDeleteTabCPPTConfirm"
                                                                                            FocusedRowChanged="GridTabCPPT_FocusedRowChanged">
                                                                                        <Columns>
                                                                                            <DxGridDataColumn FieldName="Title" />
                                                                                            <DxGridDataColumn FieldName="Body" />
                                                                                            @if (GeneralConsultanService.Status.Equals(EnumStatusGeneralConsultantService.NurseStation) || GeneralConsultanService.Status.Equals(EnumStatusGeneralConsultantService.Physician))
                                                                                            {
                                                                                                <DxGridCommandColumn NewButtonVisible="false" EditButtonVisible="false" Width="160px" />
                                                                                            }
                                                                                        </Columns>
                                                                                    </MyGrid>
                                                                                </DxLoadingPanel>
                                                                            </DxFormLayout>
                                                                        </div>

                                                                        @if (GeneralConsultanService.Status.Equals(EnumStatusGeneralConsultantService.NurseStation) || GeneralConsultanService.Status.Equals(EnumStatusGeneralConsultantService.Physician))
                                                                        {
                                                                            <div class="col-md-12 w-100">
                                                                                <div class="ml-2 mt-1 mb-2">
                                                                                    <div class="row justify-content-end">
                                                                                        <div class="col-auto align-self-end">
                                                                                            <DxButton Text="Save CPPT"
                                                                                                      Click="OnClickSaveCPPT"
                                                                                                      RenderStyle="ButtonRenderStyle.Primary"
                                                                                                      IconCssClass="fa-solid fa-floppy-disk"
                                                                                                      CssClass="w-20 ml" />
                                                                                            <DxButton Text="Clear All"
                                                                                                      RenderStyle="ButtonRenderStyle.Danger"
                                                                                                      Click="OnClickClearAllCPPT"
                                                                                                      IconCssClass="fa-solid fa-xmark"
                                                                                                      CssClass="w-20 ml" />
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        }
                                                                    </div>
                                                                </div>
                                                            </div>

                                                        </DxTabPage>

                                                        @if (IsStatus(EnumStatusGeneralConsultantService.Physician))
                                                        {
                                                            <DxTabPage Text="BPJS Integration">
                                                                <div class="w-100 mt-2">
                                                                    <div class="card">
                                                                        <div class="card-header">
                                                                            <span class="fw-bolder">BPJS Integration</span>
                                                                        </div>

                                                                        <div class="card-body">
                                                                            <div class="row w-100">
                                                                                <div class="col-md-12">
                                                                                    <DxFormLayout CssClass="w-100">
                                                                                        <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="Refer Number" ColSpanMd="12">
                                                                                            <label>-</label>
                                                                                        </DxFormLayoutItem>
                                                                                        <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="PPK Refer" ColSpanMd="12">
                                                                                            <label>@GeneralConsultanService.PPKRujukanName</label>
                                                                                        </DxFormLayoutItem>
                                                                                        <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="Spesialis/Subspesialis" ColSpanMd="12">
                                                                                            <label>@($"{GeneralConsultanService.ReferVerticalSpesialisParentSpesialisName}/{GeneralConsultanService.ReferVerticalSpesialisParentSubSpesialisName}")</label>
                                                                                        </DxFormLayoutItem>
                                                                                        <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="Note" ColSpanMd="12">
                                                                                            <label>@GeneralConsultanService.ReferReason</label>
                                                                                        </DxFormLayoutItem>
                                                                                        <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="Refer Date Visit" ColSpanMd="12">
                                                                                            <label>@GeneralConsultanService.ReferDateVisit.GetValueOrDefault().ToString("dd-MM-yyyy")</label>
                                                                                        </DxFormLayoutItem>
                                                                                    </DxFormLayout>
                                                                                </div>

                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </DxTabPage>
                                                        }
                                                    </DxTabs>
                                                </div>
                                            </div>
                                        </div>
                                    }
                                </EditForm>

                            </DxLoadingPanel>
                        }
                    </div>
                </div>
            </div>
        </section>
    </div>
}
else
{
    <LoadingIndicatorLayout />
}

@* POP UP *@

<DxPopup HeaderText="Follow Up" @bind-Visible="@IsFollowUp" HeaderCssClass="fw-bold" CloseOnOutsideClick="false" Closed="CloseFollowUpClick" Width="90%">

    <DxLoadingPanel @bind-Visible="IsLoadingFollowUp"
                    IndicatorAnimationType="WaitIndicatorAnimationType.Pulse"
                    IsContentBlocked="true"
                    ApplyBackgroundShading="true"
                    IndicatorAreaVisible="false"
                    Text="Fetching Data...">
        <EditForm Model="@FollowUpGeneralConsultanService" Context="EditFormContext2" OnValidSubmit="@HandleValidSubmitFollowUp" OnInvalidSubmit="@HandleInvalidSubmit">
            <DataAnnotationsValidator />

            <div class="card ">
                <div class="card-body">
                    <div class="w-100">
                        <div class="row w-100">
                            <div class="col-md-12 align-self-center">
                                <DxButton Enabled="!IsLoading" RenderStyle="ButtonRenderStyle.Primary" RenderStyleMode="@ButtonRenderStyleMode.Contained" IconCssClass="fa-solid fa-floppy-disk" Text="Save" SubmitFormOnClick="true"></DxButton>
                                <DxButton RenderStyle="ButtonRenderStyle.Danger" RenderStyleMode="@ButtonRenderStyleMode.Contained" IconCssClass="fa-solid fa-xmark" Text="Discard" Click="@(() => IsFollowUp = false)"></DxButton>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="w-100">
                <div class="w-100">
                    <div class="card">
                        <div class="card-header">
                            <span class="fw-bolder">General Consultation Service Information</span>
                        </div>

                        <div class="card-body">
                            <div class="row">
                                <div class="col-md-6">
                                    @{
                                        if (FollowUpGeneralConsultanService.Patient is null)
                                        {
                                            FollowUpGeneralConsultanService.Patient = new();
                                            ToastService.ShowInfo("FollowUpGeneralConsultanService.Patient Null");
                                        }
                                    }
                                    <DxFormLayout>
                                        <DxFormLayoutItem CaptionCssClass="required-caption normal-caption normal-caption" Caption="Patient" ColSpanMd="12">
                                            <MyDxComboBox Data="@Patients"
                                                          ReadOnly
                                                          NullText="Select Patient..."
                                                          @bind-Value="@FollowUpGeneralConsultanService.PatientId"
                                                          TextFieldName="Name"
                                                          SelectedItemChanged="@((UserDto e) => SelectedItemPatientChangedFollowUp(e))"
                                                          ValueFieldName="Id">
                                            </MyDxComboBox>
                                            <ValidationMessage For="@(() => FollowUpGeneralConsultanService.PatientId)"   />
                                        </DxFormLayoutItem>
                                        <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="Medical Record" ColSpanMd="12">
                                            <DxTextBox @bind-Text="(FollowUpGeneralConsultanService.Patient.NoRm)" NullText="Medical Record Number" ReadOnly InputCssClass="fw-normal" />

                                        </DxFormLayoutItem>
                                        <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="Identity Number" ColSpanMd="12">
                                            <DxTextBox @bind-Text="(FollowUpGeneralConsultanService.Patient.NoId)" NullText="Identity Number" ReadOnly InputCssClass="fw-normal" />
                                        </DxFormLayoutItem>
                                        <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="Age" ColSpanMd="12">
                                            <Template>
                                                <DxGridLayout>
                                                    <Rows>
                                                        <DxGridLayoutRow />
                                                    </Rows>
                                                    <Columns>
                                                        <DxGridLayoutColumn Width="90%" />
                                                        <DxGridLayoutColumn Width="10%" />
                                                    </Columns>
                                                    <Items>
                                                        <DxGridLayoutItem Row="0" Column="0" CssClass="mt">
                                                            <Template>
                                                                <DxSpinEdit MinValue="0" @bind-Value="@FollowUpGeneralConsultanService.Age" NullText="Age" ReadOnly InputCssClass="fw-normal" />
                                                            </Template>
                                                        </DxGridLayoutItem>
                                                        <DxGridLayoutItem Row="0" Column="1" CssClass="mt ml">
                                                            <Template>
                                                                <h6 class="pl-1 pt-2 normal-caption">Year</h6>
                                                            </Template>
                                                        </DxGridLayoutItem>
                                                    </Items>
                                                </DxGridLayout>

                                            </Template>
                                        </DxFormLayoutItem>
                                        <DxFormLayoutItem CaptionCssClass="required-caption normal-caption" Caption="Reg Type" ColSpanMd="12">
                                            <MyDxComboBox ReadOnly="@(!FollowUpGeneralConsultanService.Status.Equals(EnumStatusGeneralConsultantService.Planned))"
                                                          Data="@RegisType"
                                                          NullText="Select Registration..."
                                                          AllowUserInput="true"
                                                          SelectedItemChanged="@((string city) => SelectedItemRegisTypeChangedFollowUp(city))"
                                                          @bind-Value="@FollowUpGeneralConsultanService.TypeRegistration">
                                            </MyDxComboBox>
                                            <ValidationMessage For="@(() => FollowUpGeneralConsultanService.TypeRegistration)" />
                                        </DxFormLayoutItem>
                                        <DxFormLayoutItem Visible="@(!string.IsNullOrWhiteSpace(FollowUpGeneralConsultanService.TypeRegistration) && !FollowUpGeneralConsultanService.TypeRegistration.Equals("General Consultation"))"
                                                          CaptionCssClass="required-caption normal-caption"
                                                          Caption="Medical Type" ColSpanMd="12">
                                            <MyDxComboBox ReadOnly="@(!FollowUpGeneralConsultanService.Status.Equals(EnumStatusGeneralConsultantService.Planned))"
                                                          Data="@Method"
                                                          NullText="Select Method..."
                                                          @bind-Value="@FollowUpGeneralConsultanService.TypeMedical">
                                            </MyDxComboBox>
                                        </DxFormLayoutItem>

                                        <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="Alert Information Special Case" ColSpanMd="12">
                                            <DxCheckBox ReadOnly="@(!FollowUpGeneralConsultanService.Status.Equals(EnumStatusGeneralConsultantService.Planned))" @bind-Checked="@FollowUpGeneralConsultanService.IsAlertInformationSpecialCase" />
                                        </DxFormLayoutItem>
                                        <DxFormLayoutItem Visible="@(FollowUpGeneralConsultanService.Status.Equals(EnumStatusGeneralConsultantService.Physician))" CaptionCssClass="required-caption normal-caption" Caption="Return Status" ColSpanMd="12">
                                            <MyDxComboBox Data="@_homeStatusTemps"
                                                          NullText="Return Status..."
                                                          ReadOnly="@(!FollowUpGeneralConsultanService.Status.Equals(EnumStatusGeneralConsultantService.Physician) || FollowUpGeneralConsultanService.Status.Equals(EnumStatusGeneralConsultantService.Finished))"
                                                          @bind-Value="@FollowUpGeneralConsultanService.HomeStatus"
                                                          TextFieldName="Name"
                                                          ValueFieldName="Code">
                                            </MyDxComboBox>
                                        </DxFormLayoutItem>
                                    </DxFormLayout>
                                </div>
                                <div class="col-md-6">
                                    <DxFormLayout>
                                        <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="Patient Type" ColSpanMd="12">
                                            <MyDxComboBox Data="@ClassTypes"
                                                          NullText="Select Patient Type.."
                                                          ReadOnly="@(!FollowUpGeneralConsultanService.Status.Equals(EnumStatusGeneralConsultantService.Planned))"
                                                          TextFieldName="Name" ValueFieldName="Id"
                                                          @bind-Value="@FollowUpGeneralConsultanService.ClassTypeId">
                                            </MyDxComboBox>
                                            <ValidationMessage For="@(() => FollowUpGeneralConsultanService.ClassTypeId)" />
                                        </DxFormLayoutItem>
                                        <DxFormLayoutItem CaptionCssClass="required-caption normal-caption" Caption="Service" ColSpanMd="12">
                                            <MyDxComboBox Data="@Services"
                                                          NullText="Select Service.."
                                                          ReadOnly="@(!FollowUpGeneralConsultanService.Status.Equals(EnumStatusGeneralConsultantService.Planned))"
                                                          SelectedItemChanged="@((ServiceDto e) => SelectedItemServiceChangedFollowUp(e))"
                                                          @bind-Value="@FollowUpGeneralConsultanService.ServiceId" TextFieldName="Name" ValueFieldName="Id">
                                            </MyDxComboBox>
                                            <ValidationMessage For="@(() => FollowUpGeneralConsultanService.ServiceId)" />
                                        </DxFormLayoutItem>
                                        <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="Physicion" ColSpanMd="12">
                                            <MyDxComboBox Data="@Physicions"
                                                          NullText="Select Physicion..."
                                                          @bind-Value="@FollowUpGeneralConsultanService.PratitionerId"
                                                          ReadOnly="@(!FollowUpGeneralConsultanService.Status.Equals(EnumStatusGeneralConsultantService.Planned))"
                                                          TextFieldName="Name"
                                                          ValueFieldName="Id"
                                                          InputId="CbDoctoreName">
                                            </MyDxComboBox>
                                            <ValidationMessage For="@(() => FollowUpGeneralConsultanService.PratitionerId)" />
                                        </DxFormLayoutItem>
                                        <DxFormLayoutItem CaptionCssClass="required-caption normal-caption" Caption="Payment Method" ColSpanMd="12">
                                            <MyDxComboBox Data="@Payments"
                                                          NullText="Payment Method.."
                                                          ReadOnly="@(!FollowUpGeneralConsultanService.Status.Equals(EnumStatusGeneralConsultantService.Planned))"
                                                          SelectedItemChanged="@((string e) => SelectedItemPaymentChangedFollowUp(e))"
                                                          @bind-Value="FollowUpGeneralConsultanService.Payment">

                                            </MyDxComboBox>
                                            <ValidationMessage For="@(() => FollowUpGeneralConsultanService.Payment)" />
                                        </DxFormLayoutItem>
                                        @if (FollowUpGeneralConsultanService.Payment == "Insurance" || FollowUpGeneralConsultanService.Payment == "BPJS")
                                        {
                                            <DxFormLayoutItem CaptionCssClass="required-caption normal-caption" Caption="Insurance Policy" ColSpanMd="12">
                                                <MyDxComboBox ReadOnly="@(!FollowUpGeneralConsultanService.Status.Equals(EnumStatusGeneralConsultantService.Planned))"
                                                              Data="@FollowUpInsurancePolicies"
                                                              SelectedItemChanged="@((InsurancePolicyDto? e) => SelectedItemInsurancePolicyChangedFollowUp(e))"
                                                              @bind-Value="@SelectedInsurancePolicyFollowUp"
                                                              NullText="Select Insurance Policy..."
                                                              EditFormat="{0} - {1}">
                                                    <Columns>
                                                        <DxListEditorColumn FieldName="@nameof(InsurancePolicyDto.PolicyNumber)"
                                                                            Caption="No Card" />

                                                        <DxListEditorColumn FieldName="Insurance.Name"
                                                                            Caption="Insurance Company" />
                                                    </Columns>
                                                </MyDxComboBox>
                                            </DxFormLayoutItem>
                                        }

                                        <DxFormLayoutItem CaptionCssClass="required-caption normal-caption" Caption="Follow Up" ColSpanMd="12">
                                            <DxDateEdit @bind-Date="@FollowUpGeneralConsultanService.AppointmentDate"
                                                        Mask="@DateTimeMask.ShortDate"
                                                        NullText="Select Follow Up Date">
                                            </DxDateEdit>
                                            <ValidationMessage For="@(() => FollowUpGeneralConsultanService.AppointmentDate)" />
                                        </DxFormLayoutItem>
                                    </DxFormLayout>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </EditForm>
    </DxLoadingPanel>
</DxPopup>

<DxPopup HeaderText="Appointment pending in patient alert" @bind-Visible="@IsAppoimentPendingAlert" HeaderCssClass="fw-bold" CloseOnOutsideClick="false" Width="90%">
    <DxLoadingPanel @bind-Visible="PanelVisible"
                    IndicatorAnimationType="WaitIndicatorAnimationType.Pulse"
                    IsContentBlocked="true"
                    ApplyBackgroundShading="true"
                    IndicatorAreaVisible="false"
                    Text="Fetching Data...">
        <MyGrid @ref="Grid"
                Data="@(GeneralConsultanServices.Where(x => x.PatientId == GeneralConsultanService.PatientId && x.Status.Equals(EnumStatusGeneralConsultantService.Planned)).OrderBy(x => x.RegistrationDate))"
                KeyFieldName="Id"
                @bind-SelectedDataItems="@SelectedDataItems"
                PageSize="10">
            <Columns>
                <DxGridDataColumn FieldName="RegistrationDate" DisplayFormat="ddd, dd MMM yyyy" Caption="Registration Date" />
                <DxGridDataColumn FieldName="Patient.Name" Caption="Patient" />
                <DxGridDataColumn FieldName="Payment" Caption="Payment Methode" />
                <DxGridDataColumn FieldName="Pratitioner.Name" Caption="Doctor" />
                <DxGridDataColumn FieldName="Service.Name" Caption="Service" />
