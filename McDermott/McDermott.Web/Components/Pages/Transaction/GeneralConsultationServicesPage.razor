                            Data="@InsurancePolicies"
                            SelectedItemChanged="@((InsurancePolicyDto? e) => SelectedItemInsurancePolicyChanged(e))"
                            @bind-Value="@SelectedInsurancePolicy"
                            NullText="Select Insurance Policy..."
                            EditFormat="{0} - {1}">
                            <Columns>
