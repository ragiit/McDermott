                                                                                                    <DxGridLayoutRow />
                                                                                                </Rows>
                                                                                                <Columns>
                                                                                                    <DxGridLayoutColumn />
                                                                                                    <DxGridLayoutColumn />
                                                                                                </Columns>
                                                                                                <Items>
                                                                                                    <DxGridLayoutItem Row="0" Column="0" CssClass="normal-caption" ColumnSpan="1">
                                                                                                        <Template>
                                                                                                            <MyDxComboBox Data="@RiskOfFalling"
