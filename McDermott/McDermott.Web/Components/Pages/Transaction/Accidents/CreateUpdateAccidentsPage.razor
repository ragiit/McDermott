@page "/clinic-service/accidents/{PageMode}"

@using McDermott.Web.Components.Layout
@using McDermott.Web.Components.Popups
@rendermode InteractiveServer

<PageTitle>McHealthCare - Accidents</PageTitle>

@if (IsAccess && UserAccessCRUID is not null && (UserAccessCRUID.IsCreate || UserAccessCRUID.IsUpdate))
{
    <div class="wrapper">

        <BaseHeaderPage Title="Accidents" OnClickBack="@(async () => NavigationManager.NavigateTo(FormUrl))" ShowBackButton="true" />

        <section class="content">
            <div class="container-fluid">
                <div class="row">
                    <div class="col">
                        <DxLoadingPanel @bind-Visible="IsLoading"
                                        IndicatorAnimationType="WaitIndicatorAnimationType.Pulse"
                                        IsContentBlocked="true"
                                        ApplyBackgroundShading="true"
                                        IndicatorAreaVisible="false"
                                        Text="Fetching Data...">
                            <EditForm Model="@GeneralConsultanService"
                                      @onkeypress="@KeyPressHandler"
                                      Context="EditFormContext22" Enhance
                                      OnValidSubmit="(async () => await HandleValidSubmit(1))"
                                      OnInvalidSubmit="@HandleInvalidSubmit">

                                <DataAnnotationsValidator />
                                <ValidationSummary />
                                <AntiforgeryToken />

                                @if (Accident.Id > 0)
                                {
                                    <div class="row justify-content-end mb-3">
                                        <div class="col-auto align-self-end">
                                            <DxTabs>
                                                <DxTab Text="@EnumStatusAccident.Draft.GetDisplayName()" Enabled="@IsStatus(EnumStatusAccident.Draft)"></DxTab>
                                                @if (Accident.AccidentLocation.Equals("Inside"))
                                                {
                                                    <DxTab Text="@EnumStatusAccident.MedicalTreatment.GetDisplayName()" Enabled="@IsStatus(EnumStatusAccident.MedicalTreatment)"></DxTab>
                                                }
                                                @if (Accident.Sent is not null && Accident.Sent.Equals("Hospital"))
                                                {
                                                    <DxTab Text="@EnumStatusAccident.HospitalizationReferral.GetDisplayName()" Enabled="@IsStatus(EnumStatusAccident.HospitalizationReferral)"></DxTab>
                                                }
                                                @if (GeneralConsultanService.Payment is not null && GeneralConsultanService.Payment.Equals("BPJS"))
                                                {
                                                    <DxTab Text="@EnumStatusAccident.PatientControlMonitoring.GetDisplayName()" Enabled="@IsStatus(EnumStatusAccident.PatientControlMonitoring)"></DxTab>
                                                }
                                                <DxTab Text="@EnumStatusAccident.Done.GetDisplayName()" Enabled="@IsStatus(EnumStatusAccident.Done)"></DxTab>
                                            </DxTabs>
                                        </div>
                                    </div>
                                }

                                <div class="card sticky">
                                    <div class="card-body">
                                        <div class=" row w-100">

                                            @if (Accident.Id > 0)
                                            {
                                                <div class="col-auto pe-0 ">
                                                    <DxButton Enabled="!IsLoading"
                                                              IconCssClass="fa-solid fa-check"
                                                              Text="@StagingText.GetDisplayName()"
                                                              Visible="@(!IsStatus(EnumStatusAccident.Done))"
                                                              Click="(async () => await HandleValidSubmit(2))"
                                                              RenderStyle="ButtonRenderStyle.Primary">
                                                    </DxButton>
                                                    <DxButton IconCssClass="fa-solid fa-print"
                                                              Text="Print"
                                                              Visible="GeneralConsultanService.Id > 0"
                                                              Click="PrintAccident"
                                                              RenderStyleMode="ButtonRenderStyleMode.Outline"
                                                              RenderStyle=" ButtonRenderStyle.Success">
                                                    </DxButton>
                                                </div>
                                            }

                                            @if (!IsStatus(EnumStatusAccident.Done))
                                            {
                                                <div class="col-sm align-self-end">
                                                    <div class="row justify-content-end">
                                                        <div class="col-auto">
                                                            <DxButton Enabled="!IsLoading" RenderStyle="ButtonRenderStyle.Primary" RenderStyleMode="@ButtonRenderStyleMode.Contained" IconCssClass="fa-solid fa-floppy-disk" Text="Save" SubmitFormOnClick="true"></DxButton>
                                                            <DxButton RenderStyle="ButtonRenderStyle.Danger" RenderStyleMode="@ButtonRenderStyleMode.Contained" IconCssClass="fa-solid fa-xmark" Text="Close" Click="OnCancelBack"></DxButton>
                                                        </div>
                                                    </div>
                                                </div>
                                            }
                                        </div>
                                    </div>
                                </div>

                                <div class="card">
                                    <div class="card-body">
                                        <div class="w-100">
                                            <DxToolbar Title="Actions" ItemRenderStyleMode="ToolbarRenderStyleMode.Contained" AdaptivityMinRootItemCount="2" AdaptivityAutoHideRootItems="true" AdaptivityAutoCollapseItemsToIcons="true">
                                                <Items>
                                                    <DxToolbarItem Alignment="ToolbarItemAlignment.Left" BeginGroup="true"
                                                                   IconCssClass="fa-solid fa-notes-medical"
                                                                   Click="OnClickPopUpHistoricalMedical"
                                                                   Text="Historical Medical Record Patient" />
                                                </Items>
                                            </DxToolbar>
                                        </div>
                                    </div>
                                </div>

                                @* Accident Information *@  
                                    <_BaseGeneralConsultationServiceInformationLayoutComponent IsAccident="true"
                                                                                           GeneralConsultanService="GeneralConsultanService"
                                                                                           HeaderText="Accident Information"
                                                                                           Patients="Patients"
                                                                                           OnInputPatient="OnInputPatient"
                                                                                           OnSelectedPatientChanged="SelectedItemChanged"
                                                                                           UserForm="UserForm"
                                                                                           Physicions="Physicions"
                                                                                           OnInputPhysicion="OnInputPhysicion"
                                                                                           SelectedItemChangedPhysicion="SelectedItemChangedPhysicion"
                                                                                           SelectedItemPayment="SelectedItemPaymentChanged"
                                                                                           InsurancePolicies="InsurancePolicies"
                                                                                           OnInputInsurancePolicy="OnInputInsurancePolicy"
                                                                                           SelectedItemChangedInsurancePolicy="SelectedItemChanged"
                                                                                           ReadOnlyForm="ReadOnlyForm"
                                                                                           Projects="Projects"
                                                                                           OnInputProject="OnInputProject"
                                                                                           SelectedItemProject="SelectedItemChanged"
                                                                                           SafetyPersonels="SafetyPersonels"
                                                                                           OnInputSafetyPersonel="OnInputSafetyPersonel"
                                                                                           SelectedItemSafetyPersonel="SelectedItemPersonelChanged" />

                                <div class="w-100">
                                    <div class="card">
                                        <div class="card-body">
                                            <DxTabs>
                                                @* Clinical Assesment *@  
                                                <_BaseGeneralConsultationServiceInformationLayoutComponent GeneralConsultanService="GeneralConsultanService"
                                                                                                           OnUserFormChanged="HandleUserFormChanged"
                                                                                                                 IsAccident="true"
                                                                                                           UserForm="UserForm"
                                                                                                           ReadOnlyForm="ReadOnlyForm" />

                                                <DxTabPage CssClass="w-100" Click="OnClickTabCPPT" Text="CPPT">
                                                    <div class="w-100 mt-2">
                                                        <div class="card w-100">
                                                            <div class="card-header">
                                                                <span class="fw-bolder">CPPT</span>
                                                            </div>

                                                            <div class="row w-100">
                                                                <div class="col-md-12 w-100">
                                                                    <DxFormLayout CssClass="w-100">
                                                                        <DxLoadingPanel @bind-Visible="PanelVisible"
                                                                                        IndicatorAnimationType="WaitIndicatorAnimationType.Pulse"
                                                                                        IsContentBlocked="true"
                                                                                        ApplyBackgroundShading="true"
                                                                                        IndicatorAreaVisible="false"
                                                                                        Text="Fetching Data...">
                                                                            <MyGridPaginate @ref="GridCppt"
                                                                                            Data="GeneralConsultanCPPTs"
                                                                                            @bind-SelectedDataItems="@SelectedDataItemsCPPT"
                                                                                            EditModelSaving="OnSaveCPPT"
                                                                                            DataItemDeleting="OnDeleteCPPT"
                                                                                            FocusedRowChanged="GridTabCPPT_FocusedRowChanged"
                                                                                            SearchTextChanged="OnSearchBoxChangedGridCPPT"
                                                                                            KeyFieldName="Id">

                                                                                <Columns>
                                                                                    <DxGridDataColumn FieldName="DateTime" Caption="Date" DisplayFormat="@Helper.DefaultFormatDateTime" />
                                                                                    <DxGridDataColumn FieldName="User.Name" Caption="User" />
                                                                                    <DxGridDataColumn FieldName="Subjective" />
                                                                                    <DxGridDataColumn FieldName="Objective" />
                                                                                    <DxGridDataColumn FieldName="NursingDiagnoses.Problem" Caption="Nursing Diagnoses" />
                                                                                    <DxGridDataColumn FieldName="Diagnosis.Name" Caption="Diagnosis" />
                                                                                    <DxGridDataColumn FieldName="Planning" />
                                                                                </Columns>

                                                                                <ToolbarTemplate>
                                                                                    <MyDxToolbarBase TItem="GeneralConsultanCPPTDto"
