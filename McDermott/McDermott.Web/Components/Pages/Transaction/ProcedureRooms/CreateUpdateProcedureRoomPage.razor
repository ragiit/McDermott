@page "/clinic-service/procedure-rooms/{PageMode}"
@rendermode InteractiveServer

<PageTitle>McHealthCare - Procedure Room</PageTitle>

@if (IsAccess && UserAccessCRUID is not null && (UserAccessCRUID.IsCreate || UserAccessCRUID.IsUpdate))
{
    <div class="wrapper">

        <BaseHeaderPage Title="Procedure Room" OnClickBack="@(async () => NavigationManager.NavigateTo(FormUrl))" ShowBackButton="true" />

        <section class="content">
            <div class="container-fluid">
                <div class="row">
                    <div class="col">
                        <DxLoadingPanel @bind-Visible="IsLoading"
                                        IndicatorAnimationType="WaitIndicatorAnimationType.Pulse"
                                        IsContentBlocked="true"
                                        ApplyBackgroundShading="true"
                                        IndicatorAreaVisible="false"
                                        Text="Fetching Data...">

                            <EditForm Model="@GeneralConsultanMedicalSupport"
                                      @onkeypress="@KeyPressHandler"
                                      Context="EditFormContext22" Enhance
                                      OnValidSubmit="(async () => await HandleValidSubmit(1))"
                                      OnInvalidSubmit="@HandleInvalidSubmit">

                                <DataAnnotationsValidator />
                                <ValidationSummary />
                                <AntiforgeryToken />

                                @if (!GeneralConsultanMedicalSupport.IsConfinedSpace)
                                {
                                    <div class="row justify-content-end mb-3">
                                        <div class="col-auto align-self-end">
                                            <DxTabs>
                                                <DxTab Text="Draft" Enabled="@IsStatus(EnumStatusGeneralConsultantServiceProcedureRoom.Draft)"></DxTab>
                                                <DxTab Text="In-Progress" Enabled="@IsStatus(EnumStatusGeneralConsultantServiceProcedureRoom.InProgress)"></DxTab>
                                                <DxTab Text="Finish" Enabled="@IsStatus(EnumStatusGeneralConsultantServiceProcedureRoom.Finish)"></DxTab>
                                            </DxTabs>
                                        </div>
                                    </div>
                                    @if (!IsStatus(EnumStatusGeneralConsultantServiceProcedureRoom.Finish))
                                    {
                                        <div class="card sticky">
                                            <div class="card-body">
                                                <div class=" row w-100">
                                                    <div class="col-auto pe-0 ">
                                                        <DxButton IconCssClass="fa-solid fa-check" Text="@StagingText.GetDisplayName()" Click="(async () => await HandleValidSubmit(2))" RenderStyle="ButtonRenderStyle.Primary" Enabled="@(!GeneralConsultanMedicalSupport.Status.Equals(EnumStatusGeneralConsultantServiceProcedureRoom.Finish) && !IsLoading)"></DxButton>
                                                    </div>

                                                    <div class="col-sm align-self-end">
                                                        <div class="row justify-content-end">
                                                            <div class="col-auto">
                                                                <DxButton Enabled="!IsLoading" RenderStyle="ButtonRenderStyle.Primary" RenderStyleMode="@ButtonRenderStyleMode.Contained" IconCssClass="fa-solid fa-floppy-disk" Text="Save" SubmitFormOnClick="true"></DxButton>
                                                                <DxButton RenderStyle="ButtonRenderStyle.Danger" RenderStyleMode="@ButtonRenderStyleMode.Contained" IconCssClass="fa-solid fa-xmark" Text="Close"></DxButton>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    }
                                }

                                @if (GeneralConsultanMedicalSupport.IsConfinedSpace)
                                {
                                    <div class="card sticky">
                                        <div class="card-body">
                                            <div class=" row w-100">
                                                <div class="col-sm align-self-end">
                                                    <div class="row justify-content-end">
                                                        <div class="col-auto">
                                                            <DxButton Enabled="!IsLoading" RenderStyle="ButtonRenderStyle.Primary" RenderStyleMode="@ButtonRenderStyleMode.Contained" IconCssClass="fa-solid fa-floppy-disk" Text="Save" SubmitFormOnClick="true"></DxButton>
                                                            <DxButton RenderStyle="ButtonRenderStyle.Danger" RenderStyleMode="@ButtonRenderStyleMode.Contained" IconCssClass="fa-solid fa-xmark" Text="Close"></DxButton>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                }

                                <div class="row justify-content-end mb-3">
                                    <div class="col-auto align-self-end">
                                        <DxTabs>
                                            <DxTab Text="Referral from General Consultation" Enabled="@(GeneralConsultanMedicalSupport.IsConfinedSpace == false)"></DxTab>
                                            <DxTab Text="Confined Space" Enabled="@GeneralConsultanMedicalSupport.IsConfinedSpace"></DxTab>
                                        </DxTabs>
                                    </div>
                                </div>

                                @* General Information Services *@
                                <div class="w-100">
                                    <div class="w-100">
                                        <div class="card">
                                            <div class="card-header">
                                                <span class="fw-bolder">Personal Data Information</span>
                                            </div>

                                            <div class="card-body">
                                                <div class="row">
                                                    <div class="col-md-6">
                                                        @{
                                                            if (SelectedPatient is null)
                                                            {
                                                                SelectedPatient = new();
                                                            }

                                                            if (SelectedPatient.Department is null)
                                                            {
                                                                SelectedPatient.Department = new();
                                                            }

                                                            if (SelectedPatient.IdCardCountry is null)
                                                            {
                                                                SelectedPatient.IdCardCountry = new();
                                                            }

                                                            var yearsWorked2 = "-";
                                                            if (SelectedPatient.JoinDate is not null)
                                                            {
                                                                yearsWorked2 = $"{DateTime.Now.Year - SelectedPatient.JoinDate.GetValueOrDefault().Year} Years";
                                                            }
                                                        }
                                                        <DxFormLayout>
                                                            <DxFormLayoutItem CaptionCssClass="required-caption normal-caption" Caption="Name" ColSpanMd="12">
                                                                <MyDxComboBox Data="@Users"
                                                                              NullText="Select User"
                                                                              @ref="refUserComboBox"
                                                                              ReadOnly="!GeneralConsultanMedicalSupport.IsConfinedSpace"
                                                                              @bind-Value="@GeneralConsultanMedicalSupport.EmployeeId"
                                                                              SelectedItemChanged="@((UserDto? e) => SelectedItemPatientChanged(e))"
                                                                              TextFieldName="Name"
                                                                              ValueFieldName="Id"
                                                                              TextChanged="((string e) => OnInputUserChanged(e))">
                                                                    <Buttons>
                                                                        <DxEditorButton Click="OnSearchUserIndexDecrement"
                                                                                        IconCssClass="fa-solid fa-caret-left"
                                                                                        Visible="GeneralConsultanMedicalSupport.IsConfinedSpace"
                                                                                        Tooltip="Previous Index" />
                                                                        <DxEditorButton Click="OnSearchUser"
                                                                                        Visible="GeneralConsultanMedicalSupport.IsConfinedSpace"
                                                                                        IconCssClass="fa-solid fa-magnifying-glass"
                                                                                        Tooltip="Search" />
                                                                        <DxEditorButton Click="OnSearchUserIndexIncrement"
                                                                                        IconCssClass="fa-solid fa-caret-right"
                                                                                        Visible="GeneralConsultanMedicalSupport.IsConfinedSpace"
                                                                                        Tooltip="Next Index" />
                                                                    </Buttons>
                                                                    <Columns>
                                                                        <DxListEditorColumn FieldName="Name" />
                                                                        <DxListEditorColumn FieldName="Email" />
                                                                        <DxListEditorColumn FieldName="MobilePhone" Caption="Mobile Phone" />
                                                                        <DxListEditorColumn FieldName="Gender" />
                                                                        <DxListEditorColumn FieldName="DateOfBirth" Caption="Date Of Birth" />
                                                                        <DxListEditorColumn FieldName="NIP" />
                                                                        <DxListEditorColumn FieldName="Department.Name" Caption="Department" />
                                                                    </Columns>
                                                                </MyDxComboBox>
                                                                <ValidationMessage For="@(()=>GeneralConsultanMedicalSupport.EmployeeId)" />
                                                            </DxFormLayoutItem>

                                                            <DxFormLayoutItem CaptionCssClass="required-caption normal-caption" Caption="Dept." ColSpanMd="12">
                                                                <DxTextBox @bind-Text="@SelectedPatient.Department.Name" ReadOnly NullText="Dept." />
                                                            </DxFormLayoutItem>

                                                            <DxFormLayoutItem Caption="Nationality" CaptionCssClass="normal-caption" ColSpanMd="12">
                                                                <MyTextBox @bind-Text="@SelectedPatient.IdCardCountry.Name" ReadOnly
                                                                           NullText="Nationality" />
                                                            </DxFormLayoutItem>
                                                        </DxFormLayout>
                                                    </div>
                                                    <div class="col-md-6">
                                                        <DxFormLayout>
                                                            <DxFormLayoutItem Caption="Employee ID" CaptionCssClass="normal-caption" ColSpanMd="12">
                                                                <MyTextBox @bind-Text="@SelectedPatient.NIP" ReadOnly
                                                                           NullText="Employee ID" />
                                                            </DxFormLayoutItem>

                                                            <DxFormLayoutItem Caption="Years Worked" CaptionCssClass="normal-caption" ColSpanMd="12">
                                                                <MyTextBox @bind-Text="@yearsWorked2" ReadOnly
                                                                           NullText="Years Worked" />
                                                            </DxFormLayoutItem>
                                                        </DxFormLayout>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                @if (GeneralConsultanMedicalSupport.IsConfinedSpace)
                                {
                                    <div class="w-100">
                                        <div class="w-100">
                                            <div class="card">
                                                <div class="card-header">
                                                    <span class="fw-bolder">Medical History</span>
                                                </div>

                                                <div class="card-body">
                                                    <div class="row">
                                                        <div class="col-md-6">
                                                            <DxFormLayout>
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="First time entering confined space" ColSpanMd="12">
                                                                    <DxRadioGroup Items="@Options"
                                                                                  TextFieldName="Text"
                                                                                  ValueFieldName="Value"
                                                                                  @bind-Value="@GeneralConsultanMedicalSupport.IsFirstTimeEnteringConfinedSpace"
                                                                                  Layout="@RadioGroupLayout.Horizontal" />
                                                                </DxFormLayoutItem>

                                                                <DxFormLayoutItem Visible="!GeneralConsultanMedicalSupport.IsFirstTimeEnteringConfinedSpace" CaptionCssClass="normal-caption" Caption="If no, How many times">
                                                                    <DxSpinEdit ShowValidationIcon="true"
                                                                                MinValue="1" @bind-Value="@GeneralConsultanMedicalSupport.EnteringConfinedSpaceCount"
                                                                                NullText="0"
                                                                                ClearButtonDisplayMode="DataEditorClearButtonDisplayMode.Auto" />
                                                                </DxFormLayoutItem>

                                                                <!-- Item 1 -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="1. Defective sense of smell" ColSpanMd="12">
                                                                    <DxRadioGroup Items="@Options"
                                                                                  TextFieldName="Text"
                                                                                  ValueFieldName="Value"
                                                                                  @bind-Value="@GeneralConsultanMedicalSupport.IsDefectiveSenseOfSmell"
                                                                                  Layout="@RadioGroupLayout.Horizontal" />
                                                                </DxFormLayoutItem>

                                                                <!-- Item 2 -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="2. Asthma or Lung Ailment" ColSpanMd="12">
                                                                    <DxRadioGroup Items="@Options"
                                                                                  TextFieldName="Text"
                                                                                  ValueFieldName="Value"
                                                                                  @bind-Value="@GeneralConsultanMedicalSupport.IsAsthmaOrLungAilment"
                                                                                  Layout="@RadioGroupLayout.Horizontal" />
                                                                </DxFormLayoutItem>

                                                                <!-- Item 3 -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="3. Back Pain or Limitation of Mobility" ColSpanMd="12">
                                                                    <DxRadioGroup Items="@Options"
                                                                                  TextFieldName="Text"
                                                                                  ValueFieldName="Value"
                                                                                  @bind-Value="@GeneralConsultanMedicalSupport.IsBackPainOrLimitationOfMobility"
                                                                                  Layout="@RadioGroupLayout.Horizontal" />
                                                                </DxFormLayoutItem>

                                                                <!-- Item 4 -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="4. Claustrophobia (fear of enclosure)" ColSpanMd="12">
                                                                    <DxRadioGroup Items="@Options"
                                                                                  TextFieldName="Text"
                                                                                  ValueFieldName="Value"
                                                                                  @bind-Value="@GeneralConsultanMedicalSupport.IsClaustrophobia"
                                                                                  Layout="@RadioGroupLayout.Horizontal" />
                                                                </DxFormLayoutItem>

                                                                <!-- Item 5 -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="5. Diabetes / Hypoglycemia" ColSpanMd="12">
                                                                    <DxRadioGroup Items="@Options"
                                                                                  TextFieldName="Text"
                                                                                  ValueFieldName="Value"
                                                                                  @bind-Value="@GeneralConsultanMedicalSupport.IsDiabetesOrHypoglycemia"
                                                                                  Layout="@RadioGroupLayout.Horizontal" />
                                                                </DxFormLayoutItem>

                                                                <!-- Item 6 -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="6. Eyesight Problem" ColSpanMd="12">
                                                                    <DxRadioGroup Items="@Options"
                                                                                  TextFieldName="Text"
                                                                                  ValueFieldName="Value"
                                                                                  @bind-Value="@GeneralConsultanMedicalSupport.IsEyesightProblem"
                                                                                  Layout="@RadioGroupLayout.Horizontal" />
                                                                </DxFormLayoutItem>
                                                            </DxFormLayout>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <DxFormLayout>
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" ColSpanMd="12">

                                                                </DxFormLayoutItem>

                                                                <DxFormLayoutItem Visible="GeneralConsultanMedicalSupport.IsFirstTimeEnteringConfinedSpace" CaptionCssClass="normal-caption">

                                                                </DxFormLayoutItem>

                                                                <!-- Item 7 -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="7. Fainting Spell/Seizure/Epilepsy" ColSpanMd="12">
                                                                    <DxRadioGroup Items="@Options"
                                                                                  TextFieldName="Text"
                                                                                  ValueFieldName="Value"
                                                                                  @bind-Value="@GeneralConsultanMedicalSupport.IsFaintingSpellOrSeizureOrEpilepsy"
                                                                                  Layout="@RadioGroupLayout.Horizontal" />
                                                                </DxFormLayoutItem>

                                                                <!-- Item 8 -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="8. Hearing Disorder" ColSpanMd="12">
                                                                    <DxRadioGroup Items="@Options"
                                                                                  TextFieldName="Text"
                                                                                  ValueFieldName="Value"
                                                                                  @bind-Value="@GeneralConsultanMedicalSupport.IsHearingDisorder"
                                                                                  Layout="@RadioGroupLayout.Horizontal" />
                                                                </DxFormLayoutItem>

                                                                <!-- Item 9 -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="9. Heart Disease or Disorder" ColSpanMd="12">
                                                                    <DxRadioGroup Items="@Options"
                                                                                  TextFieldName="Text"
                                                                                  ValueFieldName="Value"
                                                                                  @bind-Value="@GeneralConsultanMedicalSupport.IsHeartDiseaseOrDisorder"
                                                                                  Layout="@RadioGroupLayout.Horizontal" />
                                                                </DxFormLayoutItem>

                                                                <!-- Item 10 -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="10. High Blood Pressure" ColSpanMd="12">
                                                                    <DxRadioGroup Items="@Options"
                                                                                  TextFieldName="Text"
                                                                                  ValueFieldName="Value"
                                                                                  @bind-Value="@GeneralConsultanMedicalSupport.IsHighBloodPressure"
                                                                                  Layout="@RadioGroupLayout.Horizontal" />
                                                                </DxFormLayoutItem>

                                                                <!-- Item 11 -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="11. Lower Limbs Deformity" ColSpanMd="12">
                                                                    <DxRadioGroup Items="@Options"
                                                                                  TextFieldName="Text"
                                                                                  ValueFieldName="Value"
                                                                                  @bind-Value="@GeneralConsultanMedicalSupport.IsLowerLimbsDeformity"
                                                                                  Layout="@RadioGroupLayout.Horizontal" />
                                                                </DxFormLayoutItem>

                                                                <!-- Item 12 -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="12. Meniere's Disease or Vertigo" ColSpanMd="12">
                                                                    <DxRadioGroup Items="@Options"
                                                                                  TextFieldName="Text"
                                                                                  ValueFieldName="Value"
                                                                                  @bind-Value="@GeneralConsultanMedicalSupport.IsMeniereDiseaseOrVertigo"
                                                                                  Layout="@RadioGroupLayout.Horizontal" />
                                                                </DxFormLayoutItem>

                                                            </DxFormLayout>
                                                        </div>

                                                        <div class="col-md-12">
                                                            <DxFormLayout>
                                                                <DxFormLayoutItem Caption="Remarks on the above, if any, otherwise write NIL:" CaptionPosition="CaptionPosition.Vertical" CaptionCssClass="normal-caption" ColSpanMd="12">
                                                                    <DxMemo @bind-Text="GeneralConsultanMedicalSupport.RemarksMedicalHistory"
                                                                            Rows="5"
                                                                            TextAreaCssClass="w-100"
                                                                            CssClass="w-100"
                                                                            NullText="Remarks on the above, if any, otherwise write NIL" />
                                                                </DxFormLayoutItem>
                                                            </DxFormLayout>
                                                        </div>

                                                        <div class="col-md-6">

                                                            <DxFormLayout CssClass="border border-2 rounded-3 pb-2 mt-2">
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" CaptionPosition="CaptionPosition.Vertical" Caption="I declare that all the Information's above to be true to the best of my knowledge and belief" ColSpanMd="12">

                                                                </DxFormLayoutItem>

                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" ColSpanMd="6">
                                                                    <DxDateEdit NullText="Date" @bind-Date="@GeneralConsultanMedicalSupport.DateMedialHistory" />
                                                                </DxFormLayoutItem>

                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" ColSpanMd="12">
                                                                    <SignaturePad.SignaturePad Class="border border-2 rounded-3" @bind-Value="GeneralConsultanMedicalSupport.SignatureEmployeeImagesMedicalHistory" ClearButtonClass="btn btn-info" ShowClearButton="true" Options="_options" style="width: 100%" />
                                                                </DxFormLayoutItem>

                                                                <DxFormLayoutItem CaptionCssClass="required-caption normal-caption" Caption="Employee" ColSpanMd="12">
                                                                    <MyDxComboBox Data="@Employees1"
                                                                                  NullText="Select Employee"
                                                                                  @ref="refEmployee1ComboBox"
                                                                                  @bind-Value="@GeneralConsultanMedicalSupport.SignatureEmployeeId"
                                                                                  TextFieldName="Name"
                                                                                  ValueFieldName="Id"
                                                                                  TextChanged="((string e) => OnInputEmployee1Changed(e))">
                                                                        <Buttons>
                                                                            <DxEditorButton Click="OnSearchEmployee1IndexDecrement"
                                                                                            IconCssClass="fa-solid fa-caret-left"
                                                                                            Tooltip="Previous Index" />
                                                                            <DxEditorButton Click="OnSearchEmployee1"
                                                                                            IconCssClass="fa-solid fa-magnifying-glass"
                                                                                            Tooltip="Search" />
                                                                            <DxEditorButton Click="OnSearchEmployee1IndexIncrement"
                                                                                            IconCssClass="fa-solid fa-caret-right"
                                                                                            Tooltip="Next Index" />
                                                                        </Buttons>
                                                                        <Columns>
                                                                            <DxListEditorColumn FieldName="Name" />
                                                                            <DxListEditorColumn FieldName="Email" />
                                                                            <DxListEditorColumn FieldName="MobilePhone" Caption="Mobile Phone" />
                                                                            <DxListEditorColumn FieldName="Gender" />
                                                                            <DxListEditorColumn FieldName="DateOfBirth" Caption="Date Of Birth" />
                                                                        </Columns>
                                                                    </MyDxComboBox>
                                                                    <ValidationMessage For="@(()=>GeneralConsultanMedicalSupport.SignatureEmployeeId)" />
                                                                </DxFormLayoutItem>
                                                            </DxFormLayout>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="card">
                                                <div class="card-header">
                                                    <span class="fw-bolder">Physical Examination</span>
                                                </div>

                                                <div class="card-body">
                                                    <div class="row">
                                                        <div class="col-md-6">
                                                            <DxFormLayout>
                                                                <!-- Weight -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="Weight" ColSpanMd="12">
                                                                    <DxSpinEdit ShowValidationIcon="true"
                                                                                MinValue="1"
                                                                                @bind-Value="@GeneralConsultanMedicalSupport.Wt"
                                                                                NullText="0"
                                                                                ClearButtonDisplayMode="DataEditorClearButtonDisplayMode.Auto" />
                                                                </DxFormLayoutItem>

                                                                <!-- Blood Pressure -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="BP" ColSpanMd="12">
                                                                    <DxSpinEdit ShowValidationIcon="true"
                                                                                MinValue="1"
                                                                                @bind-Value="@GeneralConsultanMedicalSupport.Bp"
                                                                                NullText="0"
                                                                                ClearButtonDisplayMode="DataEditorClearButtonDisplayMode.Auto" />
                                                                </DxFormLayoutItem>

                                                                <!-- Height -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="Height" ColSpanMd="12">
                                                                    <DxSpinEdit ShowValidationIcon="true"
                                                                                MinValue="1"
                                                                                @bind-Value="@GeneralConsultanMedicalSupport.Height"
                                                                                NullText="0"
                                                                                ClearButtonDisplayMode="DataEditorClearButtonDisplayMode.Auto" />
                                                                </DxFormLayoutItem>

                                                                <!-- Pulse -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="Pulse" ColSpanMd="12">
                                                                    <DxSpinEdit ShowValidationIcon="true"
                                                                                MinValue="1"
                                                                                @bind-Value="@GeneralConsultanMedicalSupport.Pulse"
                                                                                NullText="0"
                                                                                ClearButtonDisplayMode="DataEditorClearButtonDisplayMode.Auto" />
                                                                </DxFormLayoutItem>
                                                            </DxFormLayout>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <DxFormLayout>
                                                                <!-- Chest Circumference -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="Chest Circumference" ColSpanMd="12">
                                                                    <DxSpinEdit ShowValidationIcon="true"
                                                                                MinValue="1"
                                                                                @bind-Value="@GeneralConsultanMedicalSupport.ChestCircumference"
                                                                                NullText="0"
                                                                                ClearButtonDisplayMode="DataEditorClearButtonDisplayMode.Auto" />
                                                                </DxFormLayoutItem>

                                                                <!-- Abdomen Circumference -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="Abd. Circumference" ColSpanMd="12">
                                                                    <DxSpinEdit ShowValidationIcon="true"
                                                                                MinValue="1"
                                                                                @bind-Value="@GeneralConsultanMedicalSupport.AbdomenCircumference"
                                                                                NullText="0"
                                                                                ClearButtonDisplayMode="DataEditorClearButtonDisplayMode.Auto" />
                                                                </DxFormLayoutItem>

                                                                <!-- Respiratory Rate -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="Respiratory Rate" ColSpanMd="12">
                                                                    <DxSpinEdit ShowValidationIcon="true"
                                                                                MinValue="1"
                                                                                @bind-Value="@GeneralConsultanMedicalSupport.RespiratoryRate"
                                                                                NullText="0"
                                                                                ClearButtonDisplayMode="DataEditorClearButtonDisplayMode.Auto" />
                                                                </DxFormLayoutItem>

                                                                <!-- Temperature -->
                                                                <DxFormLayoutItem CaptionCssClass="normal-caption" Caption="Temperature" ColSpanMd="12">
                                                                    <DxSpinEdit ShowValidationIcon="true"
                                                                                MinValue="1"
                                                                                @bind-Value="@GeneralConsultanMedicalSupport.Temperature"
                                                                                NullText="0"
                                                                                ClearButtonDisplayMode="DataEditorClearButtonDisplayMode.Auto" />
                                                                </DxFormLayoutItem>
                                                            </DxFormLayout>
                                                        </div>

                                                        <div class="col-md-12 mt-3 justify-content-center align-items-center text-center ">
                                                            <label class="center-label">PERTINENT P.E. FINDINGS</label>
                                                        </div>

                                                        <div class="col-md-12">
                                                            <DxFormLayout>
                                                                <DxFormLayoutItem Caption="EYE" CaptionCssClass="normal-caption" ColSpanMd="12">
                                                                    <DxTextBox @bind-Text="@GeneralConsultanMedicalSupport.Eye" NullText="EYE" />
                                                                </DxFormLayoutItem>

                                                                <DxFormLayoutItem Caption="EAR NOSE THROAT" CaptionCssClass="normal-caption" ColSpanMd="12">
                                                                    <DxTextBox @bind-Text="@GeneralConsultanMedicalSupport.EarNoseThroat" NullText="EAR NOSE THROAT" />
                                                                </DxFormLayoutItem>

                                                                <DxFormLayoutItem Caption="CARDIOVASKULAR" CaptionCssClass="normal-caption" ColSpanMd="12">
                                                                    <DxTextBox @bind-Text="@GeneralConsultanMedicalSupport.Cardiovascular" NullText="CARDIOVASKULAR" />
                                                                </DxFormLayoutItem>

                                                                <DxFormLayoutItem Caption="RESPIRATORY" CaptionCssClass="normal-caption" ColSpanMd="12">
                                                                    <DxTextBox @bind-Text="@GeneralConsultanMedicalSupport.Respiratory" NullText="RESPIRATORY" />
                                                                </DxFormLayoutItem>

                                                                <DxFormLayoutItem Caption="ABDOMEN" CaptionCssClass="normal-caption" ColSpanMd="12">
                                                                    <DxTextBox @bind-Text="@GeneralConsultanMedicalSupport.Abdomen" NullText="ABDOMEN" />
                                                                </DxFormLayoutItem>

                                                                <DxFormLayoutItem Caption="EXTREMITIES" CaptionCssClass="normal-caption" ColSpanMd="12">
                                                                    <DxTextBox @bind-Text="@GeneralConsultanMedicalSupport.Extremities" NullText="EXTREMITIES" />
                                                                </DxFormLayoutItem>

                                                                <DxFormLayoutItem Caption="MUSCULOSKELETAL" CaptionCssClass="normal-caption" ColSpanMd="12">
                                                                    <DxTextBox @bind-Text="@GeneralConsultanMedicalSupport.Musculoskeletal" NullText="MUSCULOSKELETAL" />
                                                                </DxFormLayoutItem>

                                                                <DxFormLayoutItem Caption="NEUROLOGIC" CaptionCssClass="normal-caption" ColSpanMd="12">
                                                                    <DxTextBox @bind-Text="@GeneralConsultanMedicalSupport.Neurologic" NullText="NEUROLOGIC" />
                                                                </DxFormLayoutItem>

                                                                <DxFormLayoutItem Caption="SPIROMETRY TEST" CaptionCssClass="normal-caption" ColSpanMd="12">
                                                                    <DxTextBox @bind-Text="@GeneralConsultanMedicalSupport.SpirometryTest" NullText="SPIROMETRY TEST" />
                                                                </DxFormLayoutItem>

                                                                <DxFormLayoutItem Caption="RESPIRATORY FIT TEST: Passed/Failed, Respiratory type" CaptionCssClass="normal-caption" ColSpanMd="6">
                                                                    <DxTextBox @bind-Text="@GeneralConsultanMedicalSupport.RespiratoryFitTest" NullText="Passed/Failed" />
                                                                </DxFormLayoutItem>

                                                                <DxFormLayoutItem ColSpanMd="6" CaptionCssClass="normal-caption" Caption="Size">
                                                                    <DxSpinEdit ShowValidationIcon="true"
                                                                                MinValue="0"
                                                                                @bind-Value="@GeneralConsultanMedicalSupport.Size"
